-- CRIANDO TABELA PRODUTOS
CREATE TABLE PRODUTOS(
	IDPRODUTO INTEGER PRIMARY KEY AUTOINCREMENT, -- A coluna IDPRODUTO é chave primária e auto-incrementada
	NOMEPROD VARCHAR(120) UNIQUE NOT NULL, -- Nome do produto, deve ser único e não pode ser nulo
	DESCPROD VARCHAR(120), -- Descrição do produto
	CODPROD VARCHAR(10), -- Código do produto
	VLRPROD FLOAT NOT NULL, -- Valor do produto
	QTDPROD INT -- Quantidade do produto
);

-- Exemplo de DDL: como excluir uma tabela
/*
 # Caso queira deletar a tabela e adicionar a mesma novamente
 # Use o comando
    DROP TABLE PRODUTOS;
 # depois adiciona a tabela novamente
*/

--CRIANDO TABELA VENDEDORES
CREATE TABLE VENDEDORES (
    IDVENDEDOR INTEGER PRIMARY KEY AUTOINCREMENT, -- Chave primária auto-incrementada para ID do vendedor
    NOMEVEND VARCHAR(100) UNIQUE -- Nome do vendedor deve ser único
);

--CRIANDO TABELA VENDAS
CREATE TABLE VENDAS(
	IDVENDAS INTEGER PRIMARY KEY AUTOINCREMENT, -- ID da venda com chave primária auto-incrementada
	IDPRODUTO INTEGER, -- ID do produto vendido
	IDVENDEDOR INTEGER, -- ID do vendedor
	VALOR FLOAT NOT NULL, -- Valor total da venda
	QTDVENDIDA INTEGER, -- Quantidade vendida
	FOREIGN KEY(IDPRODUTO) REFERENCES produtos(IDPRODUTO), -- Chave estrangeira referenciando a tabela PRODUTOS
	FOREIGN KEY(IDVENDEDOR) REFERENCES VENDEDORES(IDVENDEDOR) -- Chave estrangeira referenciando a tabela VENDEDORES
);

